<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true" android:state_enabled="true"	android:drawable="@drawable/cancel_rounded_focused" />
	<item android:state_focused="true" android:state_enabled="true"	android:drawable="@drawable/cancel_rounded_focused" />
	<item android:state_enabled="true" android:drawable="@drawable/cancel_rounded_default" />
	<item android:state_enabled="false" android:drawable="@drawable/cancel_rounded_default" />
</selector>
